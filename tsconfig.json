{
  "compilerOptions": {
    "target": "ES5",
    "sourceMap": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "strictFunctionTypes": false,
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ],
    "types": [
      "axios-mock-adapter",
      "bootstrap",
      "jquery",
      "node",
      "qs",
      "webpack-env",
      "promise-polyfill"
    ],
    "outDir": "./bundles",
    "lib": [
      "dom",
      "es5",
      "es2015.promise"
    ]
  }
}