{
  "name": "sgn-vue",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist ./dist ./dist/assets ./dist/dll",
    "cpdll": "ncp ./dll/dll ./dist/dll && ncp ./dll/styles ./dist/styles && ncp ./dll/assets/ ./dist/assets/",
    "cpassets": "ncp ./src/assets ./dist/assets",
    "dev": "node ./build/dev-server.js",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=PRO node ./build/build.js",
    "postbuild": "npm run cpdll && npm run cpassets",
    "prebuild:dev": "npm run prebuild",
    "build:dev": "cross-env NODE_ENV=DEV node ./build/build.js",
    "postbuild:dev": "npm run postbuild",
    "prebuild:test": "npm run prebuild",
    "build:test": "cross-env NODE_ENV=TEST node ./build/build.js",
    "postbuild:test": "npm run postbuild",
    "prebuild:uat": "npm run prebuild",
    "build:uat": "cross-env NODE_ENV=UAT node ./build/build.js",
    "postbuild:uat": "npm run postbuild",
    "prebuild:dll": "rimraf dll",
    "build:dll": "webpack --config=webpack.dll.config.js",
    "postbuild:dll": "npm run cpdll",
    "preinstall": "npm i cross-env ncp rimraf typescript tslint eslint -g && npm run clean",
    "postinstall": "npm run build:dll && npm run cpassets",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "promise-polyfill": "^6.0.2",
    "qs": "^6.5.0",
    "vue": "2.4.4",
    "vue-class-component": "^5.0.2",
    "vue-router": "^2.7.0"
  },
  "devDependencies": {
    "@types/axios-mock-adapter": "^1.9.0",
    "@types/bootstrap": "^3.3.36",
    "@types/node": "8.0.26",
    "@types/qs": "^6.5.1",
    "@types/webpack-env": "1.13.0",
    "awesome-typescript-loader": "^3.2.3",
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap-loader": "^2.2.0",
    "bootstrap-sass": "^3.3.7",
    "chalk": "^2.3.0",
    "connect-history-api-fallback": "^1.4.0",
    "css-loader": "^0.28.5",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.7",
    "happypack": "^4.0.0-beta.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "node-sass": "^4.5.3",
    "opn": "^5.1.0",
    "ora": "^1.3.0",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.0",
